            <p rend="align(right)">
                <placeName>Philad</placeName>
                <date>Dec 31 1834</date>
            </p>
            <p>
                <addressee>Rev I Leeser</addressee>
            </p>
            <p rend="indent(1)">To <persName>E<add>?</add> S Carey</persName> &amp; <persName
                    role="author">A. Hart</persName>
                <add>?</add></p>
            <table cols="4" rows="12">
                <row>
                    <cell>
                        <date>May 27</date>
                    </cell>
                    <cell>To <persName>Tienners<add>?</add></persName> Chemistry</cell>
                    <cell/>
                    <cell>$1 50</cell>
                </row>
                <row>
                    <cell>
                        <date>June 30</date>
                    </cell>
                    <cell>— Ileus of Polit <add>?</add> 23.4 &amp; 5</cell>
                    <cell>
                        <hi rend="underline">40</hi>
                    </cell>
                    <cell>1 20</cell>
                </row>
                <row>
                    <cell>
                        <date>Aug 25</date>
                    </cell>
                    <cell>— <persName>P<add>?</add></persName> Traveller</cell>
                    <cell>25</cell>
                    <cell/>
                </row>
                <row>
                    <cell>“</cell>
                    <cell>- <persName>Mattineaux's</persName><add>?</add> Poor
                            <persName>Larry</persName>
                        <hi rend="underline">1.2.3.</hi></cell>
                    <cell>
                        <hi rend="underline">100</hi>
                    </cell>
                    <cell>1 25</cell>
                </row>
                <row>
                    <cell>“ <date>30</date></cell>
                    <cell>— Maps <add>?</add> 39 to 50 <add>?</add> @ 50</cell>
                    <cell/>
                    <cell>6 00</cell>
                </row>
                <row>
                    <cell>
                        <date>Dec 29</date>
                    </cell>
                    <cell>— <persName>Li<add>?</add> Kumle<add>?</add></persName> on 8 <add>?</add>
                        169 to 192</cell>
                    <cell/>
                    <cell>
                        <hi rend="underline">3 50</hi>
                    </cell>
                </row>
                <row>
                    <cell/>
                    <cell/>
                    <cell/>
                    <cell>13 78</cell>
                </row>
                <row>
                    <cell/>
                    <cell>Liby Eutg<add>?</add> Knowledge Nos 43 to 49</cell>
                    <cell/>
                    <cell/>
                </row>
                <row>
                    <cell/>
                    <cell>7 Nos c 50 cts</cell>
                    <cell/>
                    <cell>
                        <hi rend="underline"> 3 50</hi>
                    </cell>
                </row>
                <row>
                    <cell/>
                    <cell/>
                    <cell/>
                    <cell>17 28</cell>
                </row>
                <row>
                    <cell/>
                    <cell>off on maps</cell>
                    <cell/>
                    <cell>
                        <hi rend="underline"> 72</hi>
                    </cell>
