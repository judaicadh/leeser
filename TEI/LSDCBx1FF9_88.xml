        <body>
            <p rend="align(right)"><placeName>Philadelphia</placeName>, <date>December 30th
                    1852</date></p>
            <p>
                <sic>
                    <addressee>Mr Lease</addressee>
                </sic>
            </p>
            <p rend="align(right)">Bought of <persName role="author">JOHN HULSEMAN</persName>,</p>
            <p rend="align(right)"><placeName>No. 2 South Eighth Street, 2d door below Market St.
                    west side</placeName>.</p>
            <table cols="4" rows="5">
                <row>
                    <cell>
                        <date>July</date>
                    </cell>
                    <cell>
                        <date>30</date>
                    </cell>
                    <cell>two pair Kepers</cell>
                    <cell>.50</cell>
                </row>
                <row>
                    <cell/>
                    <cell>
                        <date>31</date>
                    </cell>
                    <cell>one pair half <add>?</add></cell>
                    <cell>.87</cell>
                </row>
                <row>
                    <cell>
                        <date>August</date>
                    </cell>
                    <cell>
                        <date>5</date>
                    </cell>
                    <cell>one pair foothing &amp; stifling</cell>
                    <cell>3.75</cell>
                </row>
                <row>
                    <cell/>
                    <cell>
                        <date>5</date>
                    </cell>
                    <cell>one pair foothing</cell>
                    <cell>
                        <hi rend="underline">3.50<lb/></hi>
                    </cell>
                </row>
                <row>
                    <cell/>
                    <cell/>
                    <cell/>
                    <cell>$8.62</cell>
                </row>
            </table>
            <p><date>october 11th 1852</date>
                <persName>Dr. Wulffader</persName> two pair shoes $4.50</p>
        </body>
