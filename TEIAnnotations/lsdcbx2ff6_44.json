                  "body": {
                    "type": "TextualBody",
                    "value": "<p rend=\"align(right)\">\n                <placeName>United States of America</placeName>\n            </p>\n            <p rend=\"align(right)\">\n                <placeName>Philadelphia</placeName>\n                <date>19 December</date>\n            </p>\n            <p rend=\"indent(12)\">\n                <hi rend=\"underline\">\n                    <date>1859</date>\n                </hi>\n            </p>\n            <p>Monsieur</p>\n            <p rend=\"indent(2)\">Je vous prie de m'accorder votre<lb/> protection<add>?</add></p>\n            <p rend=\"indent(7)\">Vf Serviteur</p>\n            <p rend=\"indent(7)\">\n                <persName role=\"author\">Delle Siane David</persName>\n            </p>\n            <p rend=\"indent(7)\">Professeur</p>\n            <p rend=\"indent(8)\">Teacher</p>\n            <p>P.S.</p>\n            <p>I will be very obliged to you<lb/> if you let me know at <placeName>1833<lb/> Arch\n                    Street near 19<hi rend=\"sup\">th</hi></placeName> the<lb/> church place.</p>"
                  },
                  "target": "https://colenda.library.upenn.edu/items/ark:/81431/p3r20sg7p/canvas/p1"
