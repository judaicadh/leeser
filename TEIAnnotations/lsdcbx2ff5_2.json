                  "body": {
                    "type": "TextualBody",
                    "value": "<p rend=\"align(right)\">\n                <date>Monday January 25, 5618</date>\n            </p>\n            <p>\n                <addressee>Rev Isaac Leeser</addressee>\n            </p>\n            <p rend=\"indent(2)\">Sir</p>\n            <p rend=\"indent(3)\">The <persName>Revd Dr Fischell</persName> has just<lb/> informed me\n                that you decline inserting my answer to your<lb/> note of this mos number of the\n                Occident.<lb/> To save you the expense I <sic>inclose</sic> two stamps which\n                you<lb/> will please attach and return the paper</p>\n            <p rend=\"indent(7)\">Yours</p>\n            <p rend=\"indent(7)\">\n                <persName role=\"author\"><add>?</add> M. D</persName>\n            </p>"
                  },
                  "target": "https://colenda.library.upenn.edu/items/ark:/81431/p3cf9js83/canvas/p1"
